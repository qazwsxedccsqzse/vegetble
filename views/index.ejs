<%- layout('layout') %>

<div class="row margin-top-10">
  <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
  	<ol class="carousel-indicators">
      <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
      <li data-target="#carousel-example-generic" data-slide-to="1" class=""></li>
      <li data-target="#carousel-example-generic" data-slide-to="2" class=""></li>
    </ol>
    <div class="carousel-inner" role="listbox">
      <div class="item active">
          <img data-src="/img/pic1.jpg" src="/img/pic1.jpg" data-holder-rendered="true">
      </div>
      <div class="item">
          <img data-src="/img/pic2.jpg" src="/img/pic2.jpg" data-holder-rendered="true">
      </div>
      <div class="item">
          <img data-src="/img/pic3.jpg" src="/img/pic3.jpg" data-holder-rendered="true">
      </div>
    </div>
    <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
      <span class="fa-2x fa fa-chevron-left arrow-center" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
      <span class="fa-2x fa fa-chevron-right arrow-center" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>

